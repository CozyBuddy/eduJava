	private DBConn() {}
	
	private static Connection getConnection() {
		if (conn == null) {
			String id = "scott";
			String pw = "tiger";
			
			String dr = "oracle.jdbc.driver.OracleDriver";
			String jdbcurl = "jdbc:oracle:thin:@localhost:1521:xe";
		}
		return conn;
	}

}
